{#

    Template for one tab page.

#}
<div class="row">
    <div class="statistics col-lg-7 col-md-8 col-sm-10 col-xs-12">
        <div class="statistics-categories">
            {%- for category in tab.categories -%}
                {%- if category.name != 'Main' -%}
                    {# Display category content #}
                    <div class="f-nam f-nam__{{- category.decoration -}}">
                        {{- ('groups.group_id_'~category.id)|trans -}}
                    </div>
                {%- endif -%}
                {{-
                    include('@GovWikiFrontend/Partial/Government/category.html.twig')
                -}}
            {%- endfor -%}
        </div>
    </div>
</div>