{% extends "GovWikiFrontendBundle:Layout:main.html.twig" %}

{% block main %}

    <section class="governmentController">

        {% if government is defined %}

            {% set showFinStmt = government.financialStatements is defined and government.financialStatements|length > 0 %}

            <script>
                document.addEventListener('DOMContentLoaded', function() {

                    $('.prev_tab').on('click', function () {

                        var $mobileTab = $(this).closest('.mobile-tab');

                        if ($mobileTab) {
                            $mobileTab.toggleClass('active');
                            var $prevMobileTab = prevTab();
                            gotoTab($prevMobileTab);
                        }

                        function prevTab() {
                            var $prevMobileTab = $mobileTab.prev();
                            if ($prevMobileTab.length == 0) {
                                $prevMobileTab = $mobileTab.parent().children();
                                $prevMobileTab = $prevMobileTab.eq($prevMobileTab.length-1);
                            }
                            $prevMobileTab.toggleClass('active');
                            return $prevMobileTab;
                        }

                    });

                    $('.next_tab').on('click', function () {

                        var $mobileTab = $(this).closest('.mobile-tab');

                        if ($mobileTab) {
                            $mobileTab.toggleClass('active');
                            var $nextMobileTab = nextTab();
                            gotoTab($nextMobileTab);
                        }

                        function nextTab() {
                            var $nextMobileTab = $mobileTab.next();
                            if ($nextMobileTab.length == 0) {
                                $nextMobileTab = $mobileTab.parent().children().eq(0);
                            }
                            $nextMobileTab.toggleClass('active');
                            return $nextMobileTab;
                        }

                    });


                    function gotoTab($mobileTab) {
                        $mobileTab.find('a')[0].click();
                    }

                });
            </script>
            {# TMP MOBILE LAYOUT (START) #}

                <header class="row mobile-header">
                    <div class="visible-xs col-xs-12" style="padding: 0; min-height: 150px; background-image: url('/img/California.jpg') contain;">
                        {# TODO: What is it? We can refactor? #}
                        <h6 {% if environment == 'puerto_rico' -%}
                            class="puerto"
                        {%- endif -%} style="color: white;padding: 0 8px;">{{government.name}}</h6>

                        <div class="mobile-header-content">
                            {% for tabName, categories in formats %}
                            {% if tabName == 'Overview + Elected Officials' %}
                                {% for categoryName, fields in categories %}
                                    <table class="mobile-statistics-table table">
                                        {% for fieldName, format in fields %}

                                            {% if format is viewed(government) %}
                                                {% set value = government|applay_mask(fieldName, format) %}
                                                {% if not value is null %}
                                                    <tr>
                                                        {# Field name #}
                                                        <td>
                                                            {{- ('format.'~format.field)|trans -}}
                                                            {#{{- format.name -}}#}
                                                            {% if ('format.'~format.field~'.help_text')|trans != 'format.'~format.field~'.help_text' %}
                                                                <img src="{{ asset('bundles/govwikifrontend/img/help.png') }}" title="{{- ('format.'~format.field~'.help_text')|trans -}}">
                                                            {% endif %}
                                                        </td>
                                                        {# Field value #}
                                                        <td>
                                                            {{- value|display|raw -}}
                                                            {% set rankName = "#{fieldName}_rank" %}
                                                            {%- if (government.ranks[rankName] is defined) -%}
                                                                <a class='rank'
                                                                   data-field='{{- rankName -}}'
                                                                   data-mask='{{- format.mask -}}'
                                                                   title='{{- ('format.'~format.field)|trans }}'>
                                                                    ({{- government.ranks[rankName][0] }} {{ 'preposition.of'|trans }} {{ government.ranks[rankName][1] -}})
                                                                    {#({{- government.ranks[rankName][0] }} of {{ government.ranks[rankName][1] -}})#}
                                                                </a>
                                                            {%- endif -%}

                                                        </td>
                                                    </tr>
                                                {% endif %}
                                            {% endif %}

                                        {% endfor %}
                                    </table>
                                {% endfor %}
                            {% endif %}
                        {% endfor %}

                            <div class="mobile-header-icons">
                                <img src="data:image/png;base64,"
                                     style="width: 25px; height: 25px; margin: 5px 5px 5px 0;">
                                <img src="http://www.mailamengg.com/Portals/0/photo1/logo/wikipedia.png"
                                     style="width: 25px; height: 25px; margin: 5px 5px 5px 0;">
                            </div>
                        </div>

                    </div>
                </header>

                {# Tabs control #}
                <div class="row">
                    <div class="visible-xs col-xs-12" style="padding: 0">
                        <ul class="mobile-nav">
                            {% if (formats is defined) and (formats is not empty) %}
                                {% for tabName, categories in formats %}
                                    {% if categories[""] is defined %}
                                        {% set tab_id = categories[""]|first.tab_id %}
                                    {% else %}
                                        {% set tab_id = categories|first|first.tab_id %}
                                    {% endif %}

                                    <li class="{{ loop.first ? 'active' : '' }} mobile-tab">
                                        <span class="prev_tab">
                                            <i class="glyphicon glyphicon-chevron-left"></i>
                                        </span>
                                        {# TODO: Hardcoded, for new db, must be replaced #}
                                        <a class="tab_{{ tabName | replace(' ', '_') | replace('+', 'a')|lower }}"
                                           href="#mobile-{{ tabName | replace(' ', '_') | replace('+', 'a') }}">
                                                    {{ ('groups.group_id_'~tab_id)|trans }}
                                                    {#{{ tabName }}#}
                                        </a>
                                        <span class="next_tab">
                                            <i class="glyphicon glyphicon-chevron-right"></i>
                                        </span>
                                    </li>
                                {% endfor %}
                            {% endif %}

                            {% if hasSalaries %}
                                <li class="mobile-tab">
                                    <span class="prev_tab">
                                        <i class="glyphicon glyphicon-chevron-left"></i>
                                    </span>
                                    <a class="mobile-tab tab_salaries" href="#Salaries" aria-controls="Salaries" role="tab" data-toggle="tab">Salaries</a>
                                    <span class="next_tab">
                                        <i class="glyphicon glyphicon-chevron-right"></i>
                                    </span>
                                </li>
                            {% endif %}

                            {% if hasPensions %}
                                <li class="mobile-tab">
                                    <span class="prev_tab">
                                        <i class="glyphicon glyphicon-chevron-left"></i>
                                    </span>
                                    <a class="mobile-tab tab_pensions" href="#Pensions" aria-controls="Pensions" role="tab" data-toggle="tab">Pensions</a>
                                    <span class="next_tab">
                                        <i class="glyphicon glyphicon-chevron-right"></i>
                                    </span>
                                </li>
                            {% endif %}

                            {% if showFinStmt %}
                                <li class="mobile-tab">
                                    <span class="prev_tab">
                                        <i class="glyphicon glyphicon-chevron-left"></i>
                                    </span>
                                    <a class="tab_financial_statements" href="#mobile-Financial_Statements"  aria-controls="Financial_Statements" role="tab" data-toggle="tab" data-tabname="Financial Financial_Statements">{{ 'gov.financial_statements'|trans }}</a>
                                    <span class="next_tab">
                                        <i class="glyphicon glyphicon-chevron-right"></i>
                                    </span>
                                </li>
                            {% endif %}

                        </ul>
                    </div>
                </div>


                {# Tab content #}
                <div class="row">
                    <div class="visible-xs col-xs-12" style="padding: 0">

                        {% set tab_id_list = [] %}
                        {% if (formats is defined) and (formats is not empty) %}

                            {% for tabName, categories in formats %}
                                {% if categories[""] is defined %}
                                    {% set tab_id = categories[""]|first.tab_id %}
                                {% else %}
                                    {% set tab_id = categories|first|first.tab_id %}
                                {% endif %}

                                <div class="mobile-tab-pane active"
                                     id="mobile-{{ tabName | replace(' ', '_') | replace('+', 'a') }}">

                                    <div class="mobile-tab-title">
                                        {{ ('groups.group_id_'~tab_id)|trans }}
                                    </div>
                                    {#<h4>{{ tabName }}</h4>#}
                                    <br>

                                    <div class="mobile-tab-content">

                                        {# Overview + Elected Officials #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/MobileTabs:overviewElected.html.twig') }}

                                        {# Employee Compensation #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/MobileTabs:employeeCompensation.html.twig') }}

                                        {# Quality of Services #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/MobileTabs:qualityServices.html.twig') }}

                                        {# Financial Health #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/MobileTabs:financialHealth.html.twig') }}
                                    </div>
                                </div>

                            {% endfor %}

                            {% if hasSalaries %}
                                <div class="mobile-tab-pane paginate" id="Salaries">
                                    <div class="tab-pane-main">
                                        {{ render(controller('GovWikiApiBundle:V1/Government:salaries', {
                                        'government': government.id
                                        })) }}
                                    </div>
                                </div>
                            {% endif %}

                            {% if hasPensions %}
                                <div class="mobile-tab-pane paginate" id="Pensions">
                                    <div class="tab-pane-main">
                                        {{ render(controller('GovWikiApiBundle:V1/Government:pensions', {
                                        'government': government.id
                                        })) }}
                                    </div>

                                </div>
                            {% endif %}

                            {# Financial Statements (if exists) #}
                            {% if showFinStmt %}
                                <div class="mobile-tab-pane" id="mobile-Financial_Statements">

                                    {% include 'GovWikiFrontendBundle:Partial/Government/Tabs:financialStatements.html.twig' with {
                                    'financialStatements': government.financialStatements
                                    } %}

                                </div>
                            {% endif %}

                        {% else %}
                            <h3 class="text-center">No data. Please refresh page later</h3>
                        {% endif %}

                    </div>
                </div>


            {# TMP MOBILE LAYOUT (END) #}

            {# Government name & wikipedia links #}
            <nav class="row">
                <div class="hidden-xs col-md-12 text-right">

                    {% if government.wikipediaPageName %}
                        <a target="_blank" href="{{government.wikipediaPageName}}">
                            Open Wikipedia article
                            <img src="http://www.mailamengg.com/Portals/0/photo1/logo/wikipedia.png" style="width:36px; height:36px">
                        </a>
                    {% endif %}

                    {% if government.latestAuditUrl %}
                        <a target="_blank" href="{{government.latestAuditUrl}}" style="margin-left:20px;">
                            {{ 'gov.links.latest_audit'|trans }}
                            {#Latest Audit#}
                            <img src="data:image/png;base64," style="">
                        </a>
                    {% endif %}

                </div>

            </nav>

            {# Header #}
            <header class="row">
                <div class="hidden-xs col-md-12">

                    {# TODO: What is it? We can refactor? #}
                    <h3 {% if environment == 'puerto_rico' -%}
                            class="puerto"
                    {%- endif -%}>{{government.name}}</h3>

                </div>
            </header>

            {% for flash_message in app.session.flashbag.get('message_saved_success') %}
                <div class="row">
                    <div class="alert alert-success alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        {{ flash_message }}
                    </div>
                </div>
            {% endfor %}

            <section class="row">
                <div class="hidden-xs col-md-12">

                    {# Tabs control #}
                    <nav>
                        <ul class="nav nav-pills tab-titles">
                            {% if (formats is defined) and (formats is not empty) %}
                                {% dump formats %}
                                {% for tabName, categories in formats %}
                                    {% if categories[""] is defined %}
                                        {% set tab_id = categories[""]|first.tab_id %}
                                    {% else %}
                                        {% set tab_id = categories|first|first.tab_id %}
                                    {% endif %}

                                    <li class="{{ loop.first ? 'active' : '' }}">
                                        {# TODO: Hardcoded, for new db, must be replaced #}
                                        <a class="tabs tab_{{ tabName | replace(' ', '_') | replace('+', 'a')|lower }}" href="#{{ tabName | replace(' ', '_') | replace('+', 'a') }}"
                                           aria-controls="{{ tabName | replace(' ', '_') | replace('+', 'a') }}"
                                           role="tab" data-toggle="tab" data-tabname="{{ tabName }}">
                                            {{ ('groups.group_id_'~tab_id)|trans }}
                                            {#{{ tabName }}#}
                                        </a>
                                    </li>
                                {% endfor %}
                            {% endif %}

                            {% if hasSalaries %}
                                <li><a class="tabs tab_salaries" href="#Salaries" aria-controls="Salaries" role="tab" data-toggle="tab">Salaries</a></li>
                            {% endif %}

                            {% if hasPensions %}
                                <li><a class="tabs tab_pensions" href="#Pensions" aria-controls="Pensions" role="tab" data-toggle="tab">Pensions</a></li>
                            {% endif %}

                            {% if showFinStmt %}
                                <li><a class="tabs tab_financial_statements" href="#Financial_Statements"  aria-controls="Financial_Statements" role="tab" data-toggle="tab" data-tabname="Financial_Statements">{{ 'gov.financial_statements'|trans }}</a></li>
                            {% endif %}

                        </ul>
                    </nav>

                    {# Tab content #}
                    <div class="tab-content">

                        <div class="loader"></div>
                        {% set tab_id_list = [] %}
                        {% if (formats is defined) and (formats is not empty) %}

                            {% for tabName, categories in formats %}
                                {% if categories[""] is defined %}
                                    {% set tab_id = categories[""]|first.tab_id %}
                                {% else %}
                                    {% set tab_id = categories|first|first.tab_id %}
                                {% endif %}

                                <div class="tab-pane {{ loop.first ? 'active' : '' }}" id="{{ tabName | replace(' ', '_') | replace('+', 'a') }}">

                                    <h4>{{ ('groups.group_id_'~tab_id)|trans }}</h4>
                                    {#<h4>{{ tabName }}</h4>#}
                                    <br>
                                    <div class="row">
                                        {# Statistics #}
                                        <div class="statistics col-sm-8">
                                            {{ include('@GovWikiFrontend/Partial/Government/tab.html.twig') }}
                                        </div>

                                        {# Overview + Elected Officials #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/Tabs:overviewElected.html.twig') }}

                                        {# Employee Compensation #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/Tabs:employeeCompensation.html.twig') }}

                                        {# Quality of Services #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/Tabs:qualityServices.html.twig') }}

                                        {# Financial Health #}
                                        {{ include('GovWikiFrontendBundle:Partial/Government/Tabs:financialHealth.html.twig') }}
                                    </div>
                                </div>

                            {% endfor %}

                            {% if hasSalaries %}
                                <div class="tab-pane paginate" id="Salaries">
                                    <div class="tab-pane-main">
                                        {{ render(controller('GovWikiApiBundle:V1/Government:salaries', {
                                            'government': government.id
                                        }))}}
                                    </div>
                                </div>
                            {% endif %}

                            {% if hasPensions %}
                                <div class="tab-pane paginate" id="Pensions">
                                    <div class="tab-pane-main">
                                        {{ render(controller('GovWikiApiBundle:V1/Government:pensions', {
                                            'government': government.id
                                        }))}}
                                    </div>

                                </div>
                            {% endif %}

                            {# Financial Statements (if exists) #}
                            {% if showFinStmt %}
                                <div class="tab-pane" id="Financial_Statements">

                                    {% include 'GovWikiFrontendBundle:Partial/Government/Tabs:financialStatements.html.twig' with {
                                        'financialStatements': government.financialStatements
                                    } %}

                                </div>
                            {% endif %}

                        {% else %}
                            <h3 class="text-center">No data. Please refresh page later</h3>
                        {% endif %}

                    </div>

                </div>
            </section>

        {% else %}
            <h3 class="text-center">No data</h3>
        {% endif %}

        {% if app.user and environment_is_subscribable %}
            <div class="subscribe-row">
                <a href="{{-
                    govwiki_path('govwiki_frontend_government_subscribe', {
                        'government': government.id
                    })
                -}}" class="btn {% if isSubscriber -%}btn-danger unsubscribe{%- else -%}btn-success subscribe{%- endif -%}" id="subscribe">
                    {%- if isSubscriber -%}
                        Unsubscribe
                    {%- else -%}
                        Subscribe
                    {%- endif -%}
                </a>
            </div>

            <div class="message_form_container" id="chat_message_container">
                {{ form_start(message_form) }}
                {{ form_row(message_form.text, {'label': 'Message'}) }}
                {{ form_row(message_form.submit, {'attr': {'class': 'btn btn-primary'}}) }}
                {{ form_end(message_form) }}
            </div>
        {% endif %}

        <br>
        <br>

        {% if bottomText %}
            <div class="panel panel-danger notice">
                <div class="panel-body">
                    {{ bottomText|raw }}
                </div>
            </div>
            <br>
        {% endif %}

    </section>

{% endblock %}


{# Stylesheets #}
{% block stylesheets %}

    {{parent()}}

    {% stylesheets '@GovWikiFrontendBundle/Resources/public/css/government.scss' filter='scss' %}
        <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}

{% endblock %}


{# Javascripts #}
{% block javascripts %}

    {{parent()}}

    <script src="//cdnjs.cloudflare.com/ajax/libs/numeral.js/1.4.5/numeral.min.js"></script>
    <script src='http://www.google.com/jsapi'></script>

    {% if government is defined %}
        <script>
            window.gw.government = '{{ government_json|escape('js') }}';
            window.gw.urls = {};
            window.gw.urls.popover = '{{ govwiki_path('govwiki_api_v1_government_getranks', {
                'altTypeSlug': government.altTypeSlug,
                'slug': government.slug,
            }) }}';
            window.gw.urls.search = '{{ govwiki_path('govwiki_api_v1_comparison_search') }}';
            window.gw.urls.captions = '{{ govwiki_path('govwiki_api_v1_comparison_captions') }}';
            window.gw.urls.compare = '{{ govwiki_path('govwiki_api_v1_comparison_compare') }}';
        </script>
    {% endif %}

    <script src="/js/vendor/typeahead.js"></script>
    <script src="/js/common.js"></script>
    <script src="/js/government.js"></script>

{% endblock %}
