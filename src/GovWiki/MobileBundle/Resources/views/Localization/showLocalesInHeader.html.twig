{% if locale_names_list|length > 1 %}
    {% for locale in locale_names_list %}
        <li class="{{ environment }}">
            <a class="link" onclick='changeLocale("{{ locale.shortName }}", "{{ govwiki_path('govwiki_mobile_localization_changelocale') }}", "{{ current_page_route }}")' href="">
                {{- ('general.'~ locale.shortName) | trans -}}
            </a>
        </li>
    {% endfor %}
{% endif %}