
<!-- Styles for data view -->
<style>

    .pensions {
        width: 70%;
        margin: 0 auto;
    }

    .pensions .pension {
        margin-bottom: 20px;
    }

    .pensions .pension:last-child {
        margin-bottom: 0;
    }

    .pensions .pension .pension-head {
        border: 1px solid #efefef;
        border-top-left-radius: 3px;
        border-top-right-radius: 3px;
        background: #fafafa;
        padding: 10px;
    }

    .pensions .pension .pension-head h2 {
        margin: 0;
    }

    .pensions .pension .pension-body {
        border: 1px solid #efefef;
        border-top: none;
        padding: 5px;
        overflow-x: hidden;
    }

    .pensions .pension .pension-body .pension-body-item {
        margin-bottom: 5px;
    }

    .pensions .pension .pension-body .pension-body-item-name {
        font-weight: bold;
        color: #666;
    }

    .pensions .pension .pension-body.pension-body-item-name:after {
        content: ':';
        margin-right: 5px;
    }

    .pensions .loader {
        margin: 60px auto;
        font-size: 4px;
        position: relative;
        text-indent: -9999em;
        border-top: 2px solid rgba(200,200,200,0.5);
        border-right: 2px solid rgba(200,200,200,0.5);
        border-bottom: 2px solid rgba(200,200,200,0.5);
        border-left: 2px solid #0b4d70;
        -webkit-transform: translateZ(0);
        -ms-transform: translateZ(0);
        transform: translateZ(0);
        -webkit-animation: load8 0.7s infinite linear;
        animation: load8 0.7s infinite linear;
    }

    .pensions .loader,
    .pensions .loader:after {
        border-radius: 50%;
        width: 10em;
        height: 10em;
    }

    .pensions .loader-wrapper {
        width: 100px;
        overflow: hidden;
        margin: 0 auto;
        display: none;
    }

    /*
        Pensions controls.
    */
    .pensions .pensions-controls {
        margin-bottom: 20px;
        font-size: 0.7em;
        color: #666;
    }
</style>

<!-- Common controls style -->
<style>
    .pensions-control input.styled {
        height: 25px;
        padding: 0;
        box-shadow: inset 0 -1px 0 #ddd;
    }

    .pensions-control select.styled {
        height: 26px;
        padding: 0 20px 0 10px;
    }

    .pensions-control input.styled,
    .pensions-control select.styled {
        border: 0;
        border-radius: 0;
        color: #666;
        appearance: none;
        -webkit-appearance: none;
    }

    .pensions-control input.styled:focus,
    .pensions-control select.styled:focus {
        box-shadow: inset 0 -2px 0 #2196f3;
        outline: 0;
    }

    .pensions-control select.styled {
