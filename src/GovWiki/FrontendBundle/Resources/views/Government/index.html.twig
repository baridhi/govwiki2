{% extends "GovWikiFrontendBundle:Layout:main.html.twig" %}

{% block content %}

    <div id="dataContainer" class="container" style="display: block;">

        <div class="row">
            <div class="col-md-12">


                <div id="details">
                    <div id="details-inner">
                        <div style="min-height:60px;">
                            <div style="text-align:right; min-height:48px;" class="pull-right top-links-wrapper">

                                {#{% if government.wikipedia_page_name %}#}
                                    {#<a target="_blank" href="{{government.wikipedia_page_name}}">#}
                                        {#Open Wikipedia article#}
                                        {#<img src="http://www.mailamengg.com/Portals/0/photo1/logo/wikipedia.png" style="width:36px; height:36px">#}
                                    {#</a>#}
                                {#{% endif %}#}

                                {#{% if government.latest_audit_url %}#}
                                    {#<a target="_blank" href="{{government.latest_audit_url}}" style="margin-left:20px;">#}
                                        {#Latest Audit#}
                                        {#<img src="data:image/png;base64," style="">#}
                                    {#</a>#}
                                {#{% endif %}#}

                                {#{% if government.transparent_california_page_name %}#}
                                    {#<a target="_blank" href="{{government.transparent_california_page_name}}" style="margin-left:20px;">#}
                                        {#Transparent California#}
                                        {#<img src="http://us.cdn2.123rf.com/168nwm/blankstock/blankstock1409/blankstock140900218/31369868-text-edit-sign-icon-letter-t-button-circle-flat-button-with-shadow-modern-ui-website-navigation-vect.jpg" style="width:36px; height:36px">#}
                                    {#</a>#}
                                {#{% endif %}#}

                            </div>
                            {#<h3 class="county-title" style="text-transform:uppercase;padding:0; margin:0;">{{{government:title}}}</h3>#}
                        </div>

                        <div class="tabpanel">
                            <ul id="fieldTabs" class="nav nav-pills" role="tablist">

                                {# Render TABS #}
                                {#{%for tab in fusion.tabs %}#}
                                    {#<li role="presentation" class="{{tab.active}}">#}
                                        {#<a href="#{{tab.tabid}}" aria-controls="home" role="tab"#}
                                           {#data-toggle="tab" data-tabname="{{tab.tabname}}">{{tab.tabname}}</a>#}
                                    {#</li>#}
                                {#{% endfor %}#}

                            </ul>
                            <div id="tabsContent" class="tab-content">
                                <div>
                                    <span class="f-nam">Web Site Address</span>
                                    <span class="f-val"><a target="_blank" href="{{ government.webSite }}">{{ government.webSite }}</a></span>
                                </div>
                                <div>
                                    <span class="f-nam">Enrollment</span>
                                    <span class="f-val">{{ government.enrollment }}
                                        {# TODO: Connect fusion tables from our server #}
                                        <a class="rank" data-field="enrollment_rank" title="Enrollment ranks" data-mask="0,0">(776 of 1076)</a>
                                    </span>
                                </div>

                                {% for elected in government.electedOfficials %}

                                    <div style="margin-top:20px;" class="f-names">

                                        <span class="f-nam f-nam-margin">

                                            {{elected.title}}
                                            <br>
                                            <a href="/{{government.altTypeSlug}}/{{government.slug}}/{{elected.slug}}" class="elected_link">{{elected.fullName}}</a><br>

                                            {% if elected.emailAddress %}
                                                <a href="mailto:{{elected.emailAddress}}">{{elected.emailAddress}}</a><br />
                                            {% endif %}

                                            {% if elected.telephoneNumber %}
                                                {{elected.telephoneNumber}}<br />
                                            {% endif %}

                                            {{elected.termexpires}}

                                        </span>
                                        {% if elected.photoUrl %}
                                            <a href="/{{government.altTypeSlug}}/{{government.slug}}/{{elected.slug}}" class="elected_link">
                                                <span class="f-val">
                                                    <img src="{{elected.photoUrl}}" class="portrait"/>
                                                </span>
                                            </a>
                                        {% endif %}

                                    </div>

                                {% endfor %}
                            </div>
                        </div>

                        <br>
                        <br>

                        <div class="panel panel-danger notice">
                            <div class="panel-body">
                                <p class="text-center">If you would like to help us add information to complete this profile, or if you notice any information here that may be incorrect, please contact the Center at <a href="mailto:contact@californiapolicycenter.org">contact@californiapolicycenter.org</a></p>
                            </div>
                        </div>

                    </div>
                </div>


            </div>
        </div>

    </div>
{% endblock %}
