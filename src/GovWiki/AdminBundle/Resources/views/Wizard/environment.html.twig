{% extends "GovWikiAdminBundle:Layout:wizard.html.twig" %}

{% block panel_title %}
    Environment
{% endblock panel_title %}

{% block breadcrumb %}
    <li class="active">New environment</li>
{% endblock breadcrumb %}

{% block content %}
    <h3>New environment</h3>

    {{ form_start(form) }}
    {{ form_widget(form) }}

    <a class="pull-left btn btn-danger" href="{{ path('govwiki_admin_wizard_cancel') }}">
        Cancel
    </a>
    <div class="pull-right btn-group">
        <button class="btn btn-primary" type="submit">Next</button>
    </div>
    {{ form_end(form) }}
{% endblock content %}
